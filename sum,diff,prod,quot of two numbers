// c program to find sum,difference,product and quotient
#include<stdio.h>
int main(){
    float a,b;
    float sum,diff,prod,quot;
    printf("enter the value of a\n");
    scanf("%f",&a);
    printf("enter the value of b\n");
    scanf("%f",&b);
    sum = a+b;
    diff = a-b;
    prod = a*b;
    diff = a/b;
    printf("sum=%.2f\n",sum);
    printf("diff=%.2f\n",diff);
    printf("prod=%.2f\n",prod);
    printf("diff=%.2f\n",diff);
    return 0;
}
